{
  "name": "UBX-TIM-TP",
  "class_id": "0x0d",
  "message_id": "0x01",
  "message_type": "periodic_polled",
  "description": "Time pulse time data. This message contains information on the timing of the next pulse at the TIMEPULSEO output. The recommended configuration when using this message is to set both the measurement rate (CFG-RATE) and the timepulse frequency (CFG-TP) to 1 Hz.",
  "payload": {
    "length": {
      "fixed": 16
    },
    "fields": [
      {
        "name": "towMS",
        "byte_offset": 0,
        "data_type": "U4",
        "unit": "ms",
        "description": "Time pulse time of week according to time base"
      },
      {
        "name": "towSubMS",
        "byte_offset": 4,
        "data_type": "U4",
        "scale": {
          "raw": "2^-32",
          "multiplier": 2.3283064365386963e-10
        },
        "unit": "ms",
        "description": "Submillisecond part of towMS"
      },
      {
        "name": "qErr",
        "byte_offset": 8,
        "data_type": "I4",
        "unit": "ps",
        "description": "Quantization error of time pulse"
      },
      {
        "name": "week",
        "byte_offset": 12,
        "data_type": "U2",
        "unit": "weeks",
        "description": "Time pulse week number according to time base"
      },
      {
        "name": "flags",
        "byte_offset": 14,
        "data_type": "X1",
        "description": "Flags",
        "sub_fields": [
          {
            "name": "timeBase",
            "bit_offset": 0,
            "bit_length": 1,
            "description": "Time base for towMS and week",
            "values": {
              "0": "Time base is GNSS",
              "1": "Time base is UTC"
            }
          },
          {
            "name": "utc",
            "bit_offset": 1,
            "bit_length": 1,
            "description": "UTC information availability",
            "values": {
              "0": "UTC not available",
              "1": "UTC available"
            }
          },
          {
            "name": "raim",
            "bit_offset": 2,
            "bit_length": 2,
            "description": "(T)RAIM information",
            "values": {
              "0": "Information not available",
              "1": "Not active",
              "2": "Active"
            }
          },
          {
            "name": "qErrInvalid",
            "bit_offset": 4,
            "bit_length": 1,
            "description": "Quantization error validity",
            "values": {
              "0": "Quantization error valid",
              "1": "Quantization error invalid"
            }
          }
        ]
      },
      {
        "name": "refInfo",
        "byte_offset": 15,
        "data_type": "X1",
        "description": "Time reference information. GNSS reference information. Only valid if time base is GNSS (timeBase=0).",
        "sub_fields": [
          {
            "name": "timeRefGnss",
            "bit_offset": 0,
            "bit_length": 4,
            "description": "GNSS reference information. Only valid if time base is GNSS (timeBase=0).",
            "values": {
              "0": "GPS",
              "1": "GLONASS",
              "2": "BeiDou",
              "3": "Galileo"
            }
          }
        ]
      }
    ]
  }
}
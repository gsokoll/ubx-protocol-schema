{
  "name": "UBX-NAV-PVAT",
  "class_id": "0x01",
  "message_id": "0x17",
  "message_type": "periodic_polled",
  "description": "This message combines position, velocity, attitude and time solution, including accuracy figures. Note that during a leap second there may be more or less than 60 seconds in a minute. See description of leap seconds in the integration manual for details.",
  "payload": {
    "length": {
      "fixed": 116
    },
    "fields": [
      {
        "name": "iTOW",
        "byte_offset": 0,
        "data_type": "U4",
        "unit": "ms",
        "description": "GPS time of week of the navigation epoch. See section iTOW timestamps in the integration manual for details."
      },
      {
        "name": "version",
        "byte_offset": 4,
        "data_type": "U1",
        "description": "Message version (0x00 for this version)"
      },
      {
        "name": "valid",
        "byte_offset": 5,
        "data_type": "X1",
        "description": "Validity flags: bit 0 = valid UTC Date; bit 1 = valid UTC time of day; bit 2 = UTC time of day has been fully resolved (no seconds uncertainty); bit 3 = valid magnetic declination"
      },
      {
        "name": "year",
        "byte_offset": 6,
        "data_type": "U2",
        "unit": "y",
        "description": "Year (UTC)"
      },
      {
        "name": "month",
        "byte_offset": 8,
        "data_type": "U1",
        "unit": "month",
        "description": "Month, range 1..12 (UTC)"
      },
      {
        "name": "day",
        "byte_offset": 9,
        "data_type": "U1",
        "unit": "d",
        "description": "Day of month, range 1..31 (UTC)"
      },
      {
        "name": "hour",
        "byte_offset": 10,
        "data_type": "U1",
        "unit": "h",
        "description": "Hour of day, range 0..23 (UTC)"
      },
      {
        "name": "min",
        "byte_offset": 11,
        "data_type": "U1",
        "unit": "min",
        "description": "Minute of hour, range 0..59 (UTC)"
      },
      {
        "name": "sec",
        "byte_offset": 12,
        "data_type": "U1",
        "unit": "s",
        "description": "Seconds of minute, range 0..60 (UTC)"
      },
      {
        "name": "reserved0",
        "byte_offset": 13,
        "data_type": "U1",
        "description": "Reserved",
        "reserved": true
      },
      {
        "name": "reserved1",
        "byte_offset": 14,
        "data_type": {
          "array_of": "U1",
          "count": 2
        },
        "description": "Reserved",
        "reserved": true
      },
      {
        "name": "tAcc",
        "byte_offset": 16,
        "data_type": "U4",
        "unit": "ns",
        "description": "Time accuracy estimate (UTC)"
      },
      {
        "name": "nano",
        "byte_offset": 20,
        "data_type": "I4",
        "unit": "ns",
        "description": "Fraction of second, range -1e9.. 1e9 (UTC)"
      },
      {
        "name": "fixType",
        "byte_offset": 24,
        "data_type": "U1",
        "description": "GNSSfix Type: 0 = no fix, 1 = dead reckoning only, 2 = 2D-fix, 3 = 3D-fix, 4 = GNSS + dead reckoning combined, 5 = time only fix"
      },
      {
        "name": "flags",
        "byte_offset": 25,
        "data_type": "X1",
        "description": "Fix status flags: bit 0 = valid fix; bit 1 = differential corrections were applied; bit 3 = vehicle roll is valid; bit 4 = vehicle pitch is valid; bit 5 = vehicle heading is valid; bits 6-7 = Carrier range solution status (0 = no, 1 = float ambiguities, 2 = fixed ambiguities)"
      },
      {
        "name": "flags2",
        "byte_offset": 26,
        "data_type": "X1",
        "description": "Additional flags: bit 5 = UTC Date and Time of Day validity confirmation available; bit 6 = UTC Date validity confirmed; bit 7 = UTC Time of Day validity confirmed"
      },
      {
        "name": "numSV",
        "byte_offset": 27,
        "data_type": "U1",
        "description": "Number of satellites used in Nav Solution"
      },
      {
        "name": "lon",
        "byte_offset": 28,
        "data_type": "I4",
        "scale": {
          "raw": "1e-7",
          "multiplier": 1e-07
        },
        "unit": "deg",
        "description": "Longitude"
      },
      {
        "name": "lat",
        "byte_offset": 32,
        "data_type": "I4",
        "scale": {
          "raw": "1e-7",
          "multiplier": 1e-07
        },
        "unit": "deg",
        "description": "Latitude"
      },
      {
        "name": "height",
        "byte_offset": 36,
        "data_type": "I4",
        "unit": "mm",
        "description": "Height above ellipsoid"
      },
      {
        "name": "hMSL",
        "byte_offset": 40,
        "data_type": "I4",
        "unit": "mm",
        "description": "Height above mean sea level"
      },
      {
        "name": "hAcc",
        "byte_offset": 44,
        "data_type": "U4",
        "unit": "mm",
        "description": "Horizontal accuracy estimate"
      },
      {
        "name": "vAcc",
        "byte_offset": 48,
        "data_type": "U4",
        "unit": "mm",
        "description": "Vertical accuracy estimate"
      },
      {
        "name": "velN",
        "byte_offset": 52,
        "data_type": "I4",
        "unit": "mm/s",
        "description": "NED north velocity"
      },
      {
        "name": "velE",
        "byte_offset": 56,
        "data_type": "I4",
        "unit": "mm/s",
        "description": "NED east velocity"
      },
      {
        "name": "velD",
        "byte_offset": 60,
        "data_type": "I4",
        "unit": "mm/s",
        "description": "NED down velocity"
      },
      {
        "name": "gSpeed",
        "byte_offset": 64,
        "data_type": "I4",
        "unit": "mm/s",
        "description": "Ground Speed (2-D)"
      },
      {
        "name": "sAcc",
        "byte_offset": 68,
        "data_type": "U4",
        "unit": "mm/s",
        "description": "Speed accuracy estimate"
      },
      {
        "name": "vehRoll",
        "byte_offset": 72,
        "data_type": "I4",
        "scale": {
          "raw": "1e-5",
          "multiplier": 1e-05
        },
        "unit": "deg",
        "description": "Vehicle roll."
      },
      {
        "name": "vehPitch",
        "byte_offset": 76,
        "data_type": "I4",
        "scale": {
          "raw": "1e-5",
          "multiplier": 1e-05
        },
        "unit": "deg",
        "description": "Vehicle pitch."
      },
      {
        "name": "vehHeading",
        "byte_offset": 80,
        "data_type": "I4",
        "scale": {
          "raw": "1e-5",
          "multiplier": 1e-05
        },
        "unit": "deg",
        "description": "Vehicle heading."
      },
      {
        "name": "motHeading",
        "byte_offset": 84,
        "data_type": "I4",
        "scale": {
          "raw": "1e-5",
          "multiplier": 1e-05
        },
        "unit": "deg",
        "description": "Motion heading."
      },
      {
        "name": "accRoll",
        "byte_offset": 88,
        "data_type": "U2",
        "scale": {
          "raw": "1e-2",
          "multiplier": 0.01
        },
        "unit": "deg",
        "description": "Vehicle roll accuracy (if null, roll angle is not available)."
      },
      {
        "name": "accPitch",
        "byte_offset": 90,
        "data_type": "U2",
        "scale": {
          "raw": "1e-2",
          "multiplier": 0.01
        },
        "unit": "deg",
        "description": "Vehicle pitch accuracy (if null, pitch angle is not available)."
      },
      {
        "name": "reserved2",
        "byte_offset": 92,
        "data_type": {
          "array_of": "U1",
          "count": 24
        },
        "description": "Reserved",
        "reserved": true
      }
    ]
  }
}
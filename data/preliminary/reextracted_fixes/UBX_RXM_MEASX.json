{
  "name": "UBX-RXM-MEASX",
  "class_id": "0x02",
  "message_id": "0x14",
  "message_type": "periodic_polled",
  "description": "Satellite measurements for RRLP",
  "payload": {
    "length": {
      "variable": {
        "base": 44,
        "formula": "44 + numSV * 24"
      }
    },
    "fields": [
      {
        "name": "version",
        "byte_offset": 0,
        "data_type": "U1",
        "description": "Message version, currently 0x01"
      },
      {
        "name": "reserved0",
        "byte_offset": 1,
        "data_type": "U1[3]",
        "description": "Reserved"
      },
      {
        "name": "gpsTOW",
        "byte_offset": 4,
        "data_type": "U4",
        "description": "GPS measurement reference time (ms)"
      },
      {
        "name": "gloTOW",
        "byte_offset": 8,
        "data_type": "U4",
        "description": "GLONASS measurement reference time (ms)"
      },
      {
        "name": "bdsTOW",
        "byte_offset": 12,
        "data_type": "U4",
        "description": "BeiDou measurement reference time (ms)"
      },
      {
        "name": "reserved1",
        "byte_offset": 16,
        "data_type": "U1[4]",
        "description": "Reserved"
      },
      {
        "name": "qzssTOW",
        "byte_offset": 20,
        "data_type": "U4",
        "description": "QZSS measurement reference time (ms)"
      },
      {
        "name": "gpsTOWAcc",
        "byte_offset": 24,
        "data_type": "U2",
        "description": "GPS measurement reference time accuracy (scale: 2^-4 ms, 0xffff = > 4s)"
      },
      {
        "name": "gloTOWAcc",
        "byte_offset": 26,
        "data_type": "U2",
        "description": "GLONASS measurement reference time accuracy (scale: 2^-4 ms, 0xffff = > 4s)"
      },
      {
        "name": "bdsTOWAcc",
        "byte_offset": 28,
        "data_type": "U2",
        "description": "BeiDou measurement reference time accuracy (scale: 2^-4 ms, 0xffff = > 4s)"
      },
      {
        "name": "reserved2",
        "byte_offset": 30,
        "data_type": "U1[2]",
        "description": "Reserved"
      },
      {
        "name": "qzssTOWAcc",
        "byte_offset": 32,
        "data_type": "U2",
        "description": "QZSS measurement reference time accuracy (scale: 2^-4 ms, 0xffff = > 4s)"
      },
      {
        "name": "numSV",
        "byte_offset": 34,
        "data_type": "U1",
        "description": "Number of satellites in repeated block"
      },
      {
        "name": "flags",
        "byte_offset": 35,
        "data_type": "X1",
        "description": "Flags. Bitfield: bits 1..0 (U:2) towSet: TOW set (0 = no, 1 or 2 = yes)"
      },
      {
        "name": "reserved3",
        "byte_offset": 36,
        "data_type": "U1[8]",
        "description": "Reserved"
      },
      {
        "name": "repeated_block",
        "byte_offset": 44,
        "data_type": "block",
        "description": "Repeated for each satellite (numSV times)",
        "fields": [
          {
            "name": "gnssId",
            "byte_offset": 0,
            "data_type": "U1",
            "description": "GNSS ID (see Satellite Numbering)"
          },
          {
            "name": "svId",
            "byte_offset": 1,
            "data_type": "U1",
            "description": "Satellite ID (see Satellite Numbering)"
          },
          {
            "name": "CNo",
            "byte_offset": 2,
            "data_type": "U1",
            "description": "carrier noise ratio (0..63)"
          },
          {
            "name": "mpathIndic",
            "byte_offset": 3,
            "data_type": "U1",
            "description": "multipath index (according to [1]) (0 = not measured, 1 = low, 2 = medium, 3 = high)"
          },
          {
            "name": "dopplerMS",
            "byte_offset": 4,
            "data_type": "I4",
            "description": "Doppler measurement (scale: 0.04 m/s)"
          },
          {
            "name": "dopplerHz",
            "byte_offset": 8,
            "data_type": "I4",
            "description": "Doppler measurement (scale: 0.2 Hz)"
          },
          {
            "name": "wholeChips",
            "byte_offset": 12,
            "data_type": "U2",
            "description": "whole value of the code phase measurement (0..1022 for GPS)"
          },
          {
            "name": "fracChips",
            "byte_offset": 14,
            "data_type": "U2",
            "description": "fractional value of the code phase measurement (0..1023)"
          },
          {
            "name": "codePhase",
            "byte_offset": 16,
            "data_type": "U4",
            "description": "Code phase (scale: 2^-21 ms)"
          },
          {
            "name": "intCodePhase",
            "byte_offset": 20,
            "data_type": "U1",
            "description": "Integer (part of the) code phase (ms)"
          },
          {
            "name": "pseuRangeRMSErr",
            "byte_offset": 21,
            "data_type": "U1",
            "description": "pseudorange RMS error index (according to [1]) (0..63)"
          },
          {
            "name": "reserved4",
            "byte_offset": 22,
            "data_type": "U1[2]",
            "description": "Reserved"
          }
        ]
      }
    ]
  }
}
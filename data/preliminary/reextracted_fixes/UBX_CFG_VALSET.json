{
  "name": "UBX-CFG-VALSET",
  "class_id": "0x06",
  "message_id": "0x8a",
  "message_type": "set",
  "description": "Set configuration item values (with transaction)",
  "payload": {
    "length": {
      "variable": {
        "base": 4,
        "formula": "length of cfgData"
      }
    },
    "fields": [
      {
        "name": "version",
        "byte_offset": 0,
        "data_type": "U1",
        "description": "Message version (0x01 for this version)"
      },
      {
        "name": "layers",
        "byte_offset": 1,
        "data_type": "X1",
        "description": "The layers where the configuration should be applied. Bit 0: ram - Update configuration in the RAM layer; Bit 1: bbr - Update configuration in the BBR layer; Bit 2: flash - Update configuration in the Flash layer."
      },
      {
        "name": "reserved0",
        "byte_offset": 2,
        "data_type": "U1[2]",
        "description": "Reserved"
      },
      {
        "name": "cfgData",
        "byte_offset": 4,
        "data_type": "U1[]",
        "description": "Configuration data (key and value pairs)"
      }
    ]
  }
}
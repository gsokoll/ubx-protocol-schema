{
  "name": "UBX-TIM-HOC",
  "class_id": "0x0D",
  "message_id": "0x17",
  "message_type": "input",
  "description": "Host oscillator control",
  "payload": {
    "length": {
      "fixed": 8
    },
    "fields": [
      {
        "name": "version",
        "byte_offset": 0,
        "data_type": "U1",
        "fixed_value": 0,
        "description": "Message version (0x00 for this version)"
      },
      {
        "name": "oscId",
        "byte_offset": 1,
        "data_type": "U1",
        "description": "Id of oscillator: 0: internal oscillator, 1: external oscillator"
      },
      {
        "name": "flags",
        "byte_offset": 2,
        "data_type": "U1",
        "description": "Flags",
        "bitfield": [
          {
            "name": "raw",
            "bit_offset": 0,
            "num_bits": 1,
            "description": "Type of value: 0: frequency offset, 1: raw digital output"
          },
          {
            "name": "difference",
            "bit_offset": 1,
            "num_bits": 1,
            "description": "Nature of value: 0: absolute (i.e. relative to 0), 1: relative to current setting"
          }
        ]
      },
      {
        "name": "reserved1",
        "byte_offset": 3,
        "data_type": "U1",
        "reserved": true,
        "description": "Reserved"
      },
      {
        "name": "value",
        "byte_offset": 4,
        "data_type": "I4",
        "scale": {
          "raw": true,
          "multiplier": 0.00390625
        },
        "unit": "ppb",
        "description": "Required frequency offset or raw output, depending on the flags"
      }
    ]
  }
}
{
  "name": "UBX-RXM-SFRBX",
  "class_id": "0x02",
  "message_id": "0x13",
  "message_type": "output",
  "description": "This message reports a complete subframe of broadcast navigation data decoded from a single signal. The number of data words reported in each message depends on the nature of the signal.",
  "payload": {
    "length": {
      "variable": {
        "base": 8,
        "formula": "8 + numWords * 4"
      }
    },
    "fields": [
      {
        "name": "gnssId",
        "byte_offset": 0,
        "data_type": "U1",
        "description": "GNSS identifier (see Satellite Numbering)"
      },
      {
        "name": "svId",
        "byte_offset": 1,
        "data_type": "U1",
        "description": "Satellite identifier (see Satellite Numbering)"
      },
      {
        "name": "sigId",
        "byte_offset": 2,
        "data_type": "U1",
        "description": "Signal identifier (see Signal Identifiers)"
      },
      {
        "name": "freqId",
        "byte_offset": 3,
        "data_type": "U1",
        "description": "Only used for GLONASS: This is the frequency slot + 7 (range from 0 to 13)"
      },
      {
        "name": "numWords",
        "byte_offset": 4,
        "data_type": "U1",
        "description": "The number of data words contained in this message (up to 16, for currently supported signals)"
      },
      {
        "name": "chn",
        "byte_offset": 5,
        "data_type": "U1",
        "description": "The tracking channel number the message was received on"
      },
      {
        "name": "version",
        "byte_offset": 6,
        "data_type": "U1",
        "description": "Message version, (0x02 for this version)",
        "fixed_value": 2
      },
      {
        "name": "reserved0",
        "byte_offset": 7,
        "data_type": "U1",
        "description": "Reserved",
        "reserved": true
      },
      {
        "name": "dwrd",
        "byte_offset": 8,
        "data_type": {
          "array_of": "U4",
          "count_field": "numWords"
        },
        "description": "The data words"
      }
    ]
  }
}
{
  "name": "UBX-RXM-IMES",
  "class_id": "0x02",
  "message_id": "0x61",
  "message_type": "periodic_polled",
  "description": "Indoor Messaging System information",
  "payload": {
    "length": {
      "variable": {
        "base": 4,
        "formula": "4 + numTx * 44"
      }
    },
    "fields": [
      {
        "name": "numTx",
        "byte_offset": 0,
        "data_type": "U1",
        "description": "Number of transmitters contained in the message"
      },
      {
        "name": "version",
        "byte_offset": 1,
        "data_type": "U1",
        "description": "Message version (0x01 for this version)",
        "fixed_value": 1
      },
      {
        "name": "reserved1",
        "byte_offset": 2,
        "data_type": {
          "array_of": "U1",
          "count": 2
        },
        "description": "Reserved",
        "reserved": true
      },
      {
        "name": "transmitters",
        "byte_offset": 4,
        "description": "Repeated block for each transmitter",
        "repeated_block": {
          "count_field": "numTx",
          "block_length": 44,
          "fields": [
            {
              "name": "reserved2",
              "byte_offset": 0,
              "data_type": "U1",
              "description": "Reserved",
              "reserved": true
            },
            {
              "name": "txId",
              "byte_offset": 1,
              "data_type": "U1",
              "description": "Transmitter identifier"
            },
            {
              "name": "reserved3",
              "byte_offset": 2,
              "data_type": {
                "array_of": "U1",
                "count": 3
              },
              "description": "Reserved",
              "reserved": true
            },
            {
              "name": "cno",
              "byte_offset": 5,
              "data_type": "U1",
              "unit": "dBHz",
              "description": "Carrier to Noise Ratio (Signal Strength)"
            },
            {
              "name": "reserved4",
              "byte_offset": 6,
              "data_type": {
                "array_of": "U1",
                "count": 2
              },
              "description": "Reserved",
              "reserved": true
            },
            {
              "name": "doppler",
              "byte_offset": 8,
              "data_type": "I4",
              "scale": {
                "raw": "2^-12",
                "multiplier": 0.000244140625
              },
              "unit": "Hz",
              "description": "Doppler frequency with respect to 1575.4282MHz [IIIII.FFF Hz]"
            },
            {
              "name": "position1_1",
              "byte_offset": 12,
              "data_type": "X4",
              "description": "Position 1 Frame (part 1/2) (see graphic below)"
            },
            {
              "name": "position1_2",
              "byte_offset": 16,
              "data_type": "X4",
              "description": "Position 1 Frame (part 2/2) (see graphic below)"
            },
            {
              "name": "position2_1",
              "byte_offset": 20,
              "data_type": "X4",
              "description": "Position 2 Frame (part 1/3) (see graphic below)"
            },
            {
              "name": "lat",
              "byte_offset": 24,
              "data_type": "I4",
              "scale": {
                "raw": "180*2^-24",
                "multiplier": 1.072880625e-05
              },
              "unit": "deg",
              "description": "Latitude, Position 2 Frame (part 2/3)"
            },
            {
              "name": "lon",
              "byte_offset": 28,
              "data_type": "I4",
              "scale": {
                "raw": "360*2^-25",
                "multiplier": 1.072880625e-05
              },
              "unit": "deg",
              "description": "Longitude, Position 2 Frame (part 3/3)"
            },
            {
              "name": "shortIdFrame",
              "byte_offset": 32,
              "data_type": "X4",
              "description": "Short ID Frame (see graphic below)"
            },
            {
              "name": "mediumIdLSB",
              "byte_offset": 36,
              "data_type": "U4",
              "description": "Medium ID LSB, Medium ID Frame (part 1/2)"
            },
            {
              "name": "mediumId_2",
              "byte_offset": 40,
              "data_type": "X4",
              "description": "Medium ID Frame (part 2/2) (see graphic below)"
            }
          ]
        }
      }
    ]
  }
}